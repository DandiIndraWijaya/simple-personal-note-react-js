{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style":"off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-alert":"off",
        "jsx-a11y/control-has-associated-label": "off",
        "react/sort-comp": "off",
        "class-methods-use-this": "off",
        "max-len": "off"
    }
}
